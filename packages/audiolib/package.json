{
  "name": "@kid/audiolib",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@kid/schemas": "workspace:*",
    "@kid/dexie": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  }
}
