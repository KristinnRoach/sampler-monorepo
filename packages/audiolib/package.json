{
  "name": "@repo/audiolib",
  "version": "0.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly  & vite build",
    "watch": "tsc --emitDeclarationOnly --watch & vite build --watch",
    "clean": "rimraf dist && pnpm prune",
    "clean:turbo": "rimraf .turbo",
    "clean:store": "pnpm store prune && pnpm prune",
    "clean:all": "rimraf dist node_modules && pnpm store prune && pnpm prune",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "dexie": "^4.0.11"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@types/audioworklet": "^0.0.72",
    "@vitest/coverage-v8": "3.0.9",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.0"
  }
}
